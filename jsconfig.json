{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@data": ["src/data"],
      "@data/*": ["src/data/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}

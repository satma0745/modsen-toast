{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@transition": ["src/components/transition"],
      "@transition/*": ["src/components/transition/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@data": ["src/data"],
      "@data/*": ["src/data/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
